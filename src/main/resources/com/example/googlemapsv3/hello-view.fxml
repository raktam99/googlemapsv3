<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.StackPane?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.googlemapsv3.controller.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <StackPane fx:id="stckPnMaps"/>
    <TextField fx:id="txtOrigin" text="Origin"/>
    <TextField fx:id="txtWaypoints" text="Waypoints"/>
    <Button text="Get List!" onAction="#calculateOptimalRoute"/>
</VBox>
